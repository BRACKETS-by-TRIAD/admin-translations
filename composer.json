{
    "name": "brackets/admin-translations",
    "description": "Translation manager for brackets/admin-ui",
    "keywords": [
        "translations",
        "localizations",
        "manager",
        "admin",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Pavol Perd√≠k",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk/en",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/container": "5.5.*",
        "illuminate/contracts": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/console": "5.5.*",
        "brackets/admin-ui": "@dev",
        "brackets/admin-listing": "@dev",
        "brackets/translatable": "@dev"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "orchestra/database":  "~3.5",
        "phpunit/phpunit": "~6.3",
        "doctrine/dbal": "~2.5.13",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Brackets\\AdminTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\AdminTranslations\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\AdminTranslations\\AdminTranslationsServiceProvider"
            ]
        }
    }
}
