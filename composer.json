{
    "name": "brackets/admin-translations",
    "description": "Translation manager for brackets/admin-ui",
    "keywords": [
        "translations",
        "localizations",
        "manager",
        "admin",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "David Běhal",
            "email": "david.behal@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "Richard Dominik",
            "email": "richard.dominik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3.0|^8.0|^8.1",
        "illuminate/container": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "brackets/admin-ui": "dev-master",
        "brackets/admin-listing": "dev-Feat/Upgrade-Craftable-To-Laravel-10",
        "brackets/translatable": "dev-Feat/Upgrade-To-Laravel-10"
    },
    "require-dev": {
        "orchestra/testbench": "~4.0|~5.0|~6.0|~7.0|^8.0",
        "phpunit/phpunit": "^8.5|^9.0|^10.0",
        "doctrine/dbal": "~2.5.13|^2.6|^3.0|^3.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Brackets\\AdminTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\AdminTranslations\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\AdminTranslations\\AdminTranslationsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },

    "repositories": [
        {
            "type": "path",
            "url": "../admin-ui"
        },

        {
            "type": "path",
            "url": "../admin-listing"
        },

        {
            "type": "path",
            "url": "../translatable"
        }
    ]
}
